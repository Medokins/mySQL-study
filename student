CREATE TABLE student (
    student_id INT PRIMARY KEY,
    name VARCHAR(20),
    major VARCHAR(20)
);

DROP TABLE student;

DESCRIBE student;

ALTER TABLE student ADD gpa DECIMAL(3,2);

INSERT INTO student VALUES(3, 'Jim', 'Pranks', 5.5);
INSERT INTO student(student_id, name, major) VALUES(4, 'Pam', 'Art');

SELECT * FROM student;